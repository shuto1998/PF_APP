<h4><%= @customer.name %>の詳細</h4>
<div class="container">
  <div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-3">ID</div>
    <div class="col-lg-3"><%= @customer.id %></div>
    <div class="col-lg-3"></div>
  </div>
  <div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-3">名前</div>
    <div class="col-lg-3"><%= @customer.name %></div>
    <div class="col-lg-3"></div>
  </div>
  <div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-3">フリガナ</div>
    <div class="col-lg-3"><%= @customer.name_kana %></div>
    <div class="col-lg-3"></div>
  </div>
  <div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-3">住所</div>
    <div class="col-lg-3"><%= @customer.address %></div>
    <div class="col-lg-3"></div>
  </div>
  <div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-3">電話番号</div>
    <div class="col-lg-3"><%= @customer.telephone_number %></div>
    <div class="col-lg-3"></div>
  </div>
  <div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-3">メールアドレス</div>
    <div class="col-lg-3"><%= @customer.email %></div>
    <div class="col-lg-3"></div>
  </div>
  <div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-3"></div>
    <div class="col-lg-3"><%= link_to "編集する",edit_admin_customer_path(@customer.id),class: 'btn btn-success' %></div>
    <div class="col-lg-3"></div>
  </div>
</div>